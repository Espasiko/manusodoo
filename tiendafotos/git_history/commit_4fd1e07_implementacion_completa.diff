+    def create_category(self, name: str, parent_id: Optional[int] = None, 
+                       description: str = "") -> int:
+        """
+        Crea una categor√≠a de producto en Odoo
+        
+        Args:
+            name: Nombre de la categor√≠a
+            parent_id: ID de la categor√≠a padre (opcional)
+            description: Descripci√≥n de la categor√≠a
+            
+        Returns:
+            ID de la categor√≠a creada
+        """
+        # Verificar si la categor√≠a ya existe
+        domain = [('name', '=', name)]
+        if parent_id:
+            domain.append(('parent_id', '=', parent_id))
+        else:
+            domain.append(('parent_id', '=', False))
+            
+        existing_ids = self.models.execute_kw(
+            self.db, self.uid, self.password,
+            'product.category', 'search', [domain]
+        )
+        
+        if existing_ids:
+            logger.info(f"Categor√≠a '{name}' ya existe con ID {existing_ids[0]}")
+            return existing_ids[0]
+        
+        # Crear nueva categor√≠a
+        category_data = {
+            'name': name,
+            'parent_id': parent_id,
+        }
+        
+        if description:
+            category_data['property_account_income_categ_id'] = False  # Configurar seg√∫n necesidades
+            category_data['property_account_expense_categ_id'] = False  # Configurar seg√∫n necesidades
+        
+        category_id = self.models.execute_kw(
+            self.db, self.uid, self.password,
+            'product.category', 'create', [category_data]
+        )
+        
+        logger.info(f"Categor√≠a '{name}' creada con ID {category_id}")
+        return category_id
