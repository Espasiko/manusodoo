+    def get_provider_category_mapping(self) -> Dict[str, str]:
+        """
+        Mapeo de categorías de proveedores a categorías Odoo
+        
+        Returns:
+            Diccionario con el mapeo proveedor -> categoría Odoo
+        """
+        return {
